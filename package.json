{
  "name": "easier-hub-connector",
  "version": "1.0.0",
  "description": "Sistema de gestión de tokens de Facebook con auto-renovación",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "init": "npm run build && node dist/init.js",
    "start": "npm run build && node dist/index.js",
    "test": "npm run build && node dist/test.js",
    "report": "npm run build && node dist/generate-report.js",
    "upload-bq": "npm run build && node dist/upload-to-bigquery.js",
    "recreate-bq-table": "npm run build && node dist/recreate-table.js",
    "split-bq-tables": "npm run build && node dist/split-tables.js",
    "add-age": "npm run build && node dist/add-age-data.js",
    "full-demo": "npm run build && node dist/full-demographics.js",
